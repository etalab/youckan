#: static/js/common.js:10
msgid "This field is required."
msgstr "Pflichtfeld"

#: static/js/common.js:11
msgid "Please fix this field."
msgstr "Dieses Feld bitte verbessern."

#: static/js/common.js:12
msgid "Please enter a valid email address."
msgstr "Bitte geben sie eine gültige Mailadresse an."

#: static/js/common.js:13
msgid "Please enter a valid URL."
msgstr "Bitte geben sie eine gültige URL an."

#: static/js/common.js:14
msgid "Please enter a valid date."
msgstr "Bitte geben sie ein gültiges Datum ein."

#: static/js/common.js:15
msgid "Please enter a valid date (ISO)."
msgstr "Bitte geben sie ein gültiges Datum ein (ISO)."

#: static/js/common.js:16
msgid "Please enter a valid number."
msgstr "Bitte geben sie eine gültige Nummer ein."

#: static/js/common.js:17
msgid "Please enter only digits."
msgstr "Bitte geben sie nur Ziffern ein."

#: static/js/common.js:18
msgid "Please enter a valid credit card number."
msgstr "Bitte geben sie ein gültige Kreditkartennummer ein."

#: static/js/common.js:19
msgid "Please enter the same value again."
msgstr "Bitte geben sie den Wert noch einmal ein."

#: static/js/common.js:20
msgid "Please enter no more than {0} characters."
msgstr "Bitte nicht mehr als {0} Zeichen eingeben."

#: static/js/common.js:21
msgid "Please enter at least {0} characters."
msgstr "Bitte mindestens {0} Zeichen eingeben."

#: static/js/common.js:22
msgid "Please enter a value between {0} and {1} characters long."
msgstr "Bitte einen Wert eingeben, der zwischen {0} und {1} Zeichen lang ist."

#: static/js/common.js:23
msgid "Please enter a value between {0} and {1}."
msgstr "Bitte einen Wert zwischen {0} und {1}  eingeben."

#: static/js/common.js:24
msgid "Please enter a value less than or equal to {0}."
msgstr "Bitte einen Wert kleiner oder gleich {0} eingeben."

#: static/js/common.js:25
msgid "Please enter a value greater than or equal to {0}."
msgstr "Bitte einen Wert größer oder gleich {0} eingeben."
